@model ShivGardenHouse.Models.Plot

@{
    ViewBag.Title = "CancelledPlotBookingList";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

 
@using (Html.BeginForm("CancelledPlotBookingList", "Plot", FormMethod.Post, new { @id = "form1" }))
{
 
  
    <div class="page-header">
        <div class="page-block">
            <div class="row align-items-center">
                <div class="col-md-8">
                    <div class="page-header-title">
                        <h4 class="m-b-10">Cancelled Booking List</h4>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="page-wrapper">
                <div class="page-body">
                    <div class="row">
                        <div class="col-md-12">
                            <!-- HTML5 Export Buttons table start -->
                            <div class="card">
                                <div class="card-header table-card-header">

                                </div>
                                <div class="card-block">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="row">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        Customer ID <span style="color: red">*</span>
                                                        @Html.TextBoxFor(m => m.CustomerID, new { @class = "form-control", @Placeholder = "Search by Customer ID" })
                                                    </div>
                                                </div>

                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        Partner ID <span style="color: red">*</span>
                                                        @Html.TextBoxFor(m => m.AssociateID, new { @class = "form-control", @Placeholder = "Search by Partner ID" })
                                                    </div>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        Booking Number <span style="color: red">*</span>
                                                        @Html.TextBoxFor(m => m.BookingNumber, new { @class = "form-control", @Placeholder = "Search by BookingNumber  " })
                                                    </div>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <br />
                                                        <input class="btn btn-success" type="submit" value="Search" id="btnSearch" name="SearchCancelled" />
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="dt-responsive table-responsive">
                                                <table id="basic-btn" class="table table-striped table-bordered nowrap">
                                                    <thead>
                                                        <tr>
                                                            <th hidden>PK_BookingId</th>
                                                            <th>S No.</th>
                                                            <th>Branch</th>
                                                            <th>Customer Name</th>
                                                            <th>PartnerName</th>
                                                            <th>Plot No.</th>
                                                            <th>Booking Number</th>
                                                            <th>Booking Date</th>
                                                            <th>Booking Amount</th>
                                                            <th>Payment Plan</th>
                                                            <th>Remark</th>
                                                            <th>Cancel Date</th>

                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @if (Model != null)
                                                        {
                                                            if (Model.lstPlot != null)
                                                            {
                                                                int i = 1;
                                                                foreach (var Item in Model.lstPlot)
                                                                {
                                                                    <tr class="text-left">
                                                                        <td hidden>@Item.PK_BookingId</td>
                                                                        <td>@i</td>
                                                                        <td>@Item.BranchID</td>
                                                                        <td>@Item.CustomerName</td>
                                                                        <td>@Item.AssociateName</td>
                                                                        <td>@Item.PlotNumber</td>
                                                                        <td>@Item.BookingNumber</td>
                                                                        <td>@Item.BookingDate</td>
                                                                        <td>@Item.BookingAmount</td>
                                                                        <td>@Item.PaymentPlanID</td>
                                                                        <td>@Item.CancelRemark</td>
                                                                        <td>@Item.CancelDate</td>
                                                                    </tr>
                                                                    i++;
                                                                }
                                                            }
                                                        }
                                                    </tbody>
                                                    <tfoot>
                                                        <tr>
                                                            <th hidden>PK_BookingId</th>
                                                            <th>S No.</th>
                                                            <th>Branch</th>
                                                            <th>Customer Name</th>
                                                            <th>PartnerName</th>
                                                            <th>Plot No.</th>
                                                            <th>Booking Number</th>
                                                            <th>Booking Date</th>
                                                            <th>Booking Amount</th>
                                                            <th>Payment Plan</th>
                                                            <th>Remark</th>
                                                            <th>Cancel Date</th>

                                                        </tr>
                                                    </tfoot>
                                                </table>
                                            </div>

                                        </div>
                                    </div>

                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
       

               
}







